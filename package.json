{
  "name": "event-hub-app",
  "version": "1.0.0",
  "description": "MERN event hub application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-pranathi/react-event-hub.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s-pranathi/react-event-hub/issues"
  },
  "homepage": "https://github.com/s-pranathi/react-event-hub#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongodb": "^5.8.1",
    "mongoose": "^7.5.2",
    "nodemon": "^3.0.1"
  }
}
